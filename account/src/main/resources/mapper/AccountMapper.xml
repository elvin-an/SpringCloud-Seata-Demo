<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.senlin.yu.account.mapper.AccountMapper" >

    <update id="reduce">
        update account set balance = balance - #{money} where uid = #{uid}
    </update>
</mapper>
